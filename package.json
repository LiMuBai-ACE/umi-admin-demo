{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "cross-env ANALYZE=1 umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.24",
    "analyze": "^0.0.14",
    "antd": "^4.7.2",
    "compression-webpack-plugin": "^6.0.3",
    "crypto-js": "^4.0.0",
    "dva": "^2.4.1",
    "dva-core": "^2.0.3",
    "dva-loading": "^3.0.21",
    "lint-staged": "^10.0.7",
    "moment": "^2.29.1",
    "prettier": "^1.19.1",
    "react": "^16.12.0",
    "react-cookies": "^0.1.1",
    "react-dom": "^16.12.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "umi": "^3.2.24",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.0.1"
  }
}
